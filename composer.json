{
  "name": "digkill/instagram-php-crawler",
  "description": "Instagram PHP Scraper. Get account information, photos and videos without any authorization",
  "keywords": [
    "instagram",
    "scraper",
    "crawler"
  ],
  "type": "library",
  "homepage": "https://github.com/digkill/instagram-php-crawler",
  "license": "MIT",
  "authors": [
  ],
  "require": {
    "php": ">=8.0",
    "ext-curl": "*",
    "ext-json": "*",
    "psr/http-client": "~1.0",
    "guzzlehttp/guzzle": "^7.2"
  },
  "require-dev": {
    "phpunit/phpunit": "^7.0"
  },
  "autoload": {
    "psr-4": {
      "InstagramCrawler\\": "src/InstagramCrawler/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "InstagramScraper\\Tests\\": "tests/"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.x-dev"
    }
  }
}
