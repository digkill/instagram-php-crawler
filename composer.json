{
  "name": "digkill/instagram-php-crawler",
  "description": "Instagram PHP Scraper. Get account information, photos and videos without any authorization",
  "keywords": [
    "instagram",
    "scraper"
  ],
  "type": "library",
  "homepage": "https://github.com/digkill/instagram-php-crawler",
  "license": "MIT",
  "authors": [
  ],
  "require": {
    "php": ">=7.4",
    "ext-curl": "*",
    "ext-json": "*",
    "psr/simple-cache": "~1.0",
    "psr/http-client": "~1.0",
    "guzzlehttp/guzzle": "^7.2"
  },
  "require-dev": {
    "phpunit/phpunit": "^7.0",
    "phpfastcache/phpfastcache": "^7.1"
  },
  "autoload": {
    "psr-4": {
      "InstagramScraper\\": "src/InstagramScraper/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "InstagramScraper\\Tests\\": "tests/"
    }
  }
}
